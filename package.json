    "axios": "^1.3.4",
    "chart.js": "^4.4.1",
    "chartjs-to-image": "^1.2.2",
    "dayjs": "^1.11.7",
    "formik": "^2.2.9",
    "html5-qrcode": "^2.3.8",
    "jsdom": "^23.0.1",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-media-devices": "^1.1.2",
    "react-router-dom": "^6.10.0",
